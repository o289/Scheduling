<%= form_for @user do |f| %>
    <%= f.label :image %>
    <%= f.file_field :image %>

    <%= f.label :name %>
    <%= f.text_field :name %>
        
    <%= f.label :profile %>
    <%= f.text_area :profile %>

    <%= f.label :email %></p>
    <%= f.email_field :email %>
        
    <%= f.submit "OK" %>
<% end %>
